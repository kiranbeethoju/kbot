{
  "name": "kbot",
  "displayName": "KBot",
  "description": "Cursor-like AI coding assistant powered by Azure OpenAI, NVIDIA, Anthropic Foundry, and Z.AI. 100% local - no telemetry, no cloud storage. Your code never leaves your machine except for AI provider API calls.",
  "version": "1.5.0",
  "publisher": "KiranBeethojuAIplugins",
  "author": "Kiran Beethoju",
  "license": "MIT",
  "icon": "logo.png",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Machine Learning",
    "Other"
  ],
  "keywords": [
    "azure",
    "openai",
    "nvidia",
    "anthropic",
    "zai",
    "glm",
    "claude",
    "ai",
    "assistant",
    "chat",
    "coding"
  ],
  "activationEvents": [
    "onCommand:kbot.openChat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kbot.openChat",
        "title": "Open KBot Chat",
        "category": "KBot"
      },
      {
        "command": "kbot.configureCredentials",
        "title": "Configure KBot Credentials",
        "category": "KBot"
      },
      {
        "command": "kbot.showLogs",
        "title": "Show KBot Logs",
        "category": "KBot"
      },
      {
        "command": "kbot.manageExclusions",
        "title": "Manage File Exclusions",
        "category": "KBot"
      },
      {
        "command": "kbot.configureSystemPrompts",
        "title": "Configure System Prompts",
        "category": "KBot"
      },
      {
        "command": "kbot.configureWorkspace",
        "title": "Configure Workspace Directory",
        "category": "KBot"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "kbot-sidebar",
          "title": "KBot",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "kbot-sidebar": [
        {
          "type": "webview",
          "id": "kbotChatView",
          "name": "Chat",
          "contextualTitle": "KBot Chat"
        },
        {
          "type": "webview",
          "id": "kbotCredentialsView",
          "name": "Credentials",
          "contextualTitle": "KBot Credentials"
        },
        {
          "type": "webview",
          "id": "kbotHistoryView",
          "name": "History",
          "contextualTitle": "KBot Chat History"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "esbuild src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "watch": "esbuild src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --watch",
    "package": "vsce package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiranbeethoju/kbot"
  },
  "bugs": {
    "url": "https://github.com/kiranbeethoju/kbot/issues"
  },
  "homepage": "https://github.com/kiranbeethoju/kbot#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.75.0",
    "esbuild": "^0.20.0"
  }
}
